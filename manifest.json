{
  "name": "Open in default",
  "description": "Open in default browser.",
  "author": "Ilya Kantor",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
      "matches": ["*://*.slack.com/*"],
      "js": ["content-script.js"],
      "run_at": "document_start",
      "all_frames": true
  }]
}
